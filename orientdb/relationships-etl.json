         }
      },
      {
         "edge":{
            "class":"Relation",
            "joinFieldName":"_to",
            "lookup":"Profile._key",
            "unresolvedLinkAction":"ERROR"
         }
      }
   ],
   "loader":{
      "orientdb":{
         "dbURL":"plocal:@DATABASE@",
