{
   "config":{
      "parallel":false
   },
   "source":{
      "file":{
         "path":"@INPUT_RELATIONS@"
      }
   },
   "extractor":{
      "row":{

      }
   },
   "transformers":[
      {
         "csv":{
            "separator":"\t",
            "stringCharacter":"",
            "unicode":false
         }
      },
      {
         "merge":{
            "joinFieldName":"_from",
            "lookup":"Profile._key",
            "unresolvedLinkAction":"ERROR"
         }
      },
      {
